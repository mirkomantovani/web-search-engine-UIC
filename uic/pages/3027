<script type='text/javascript'>
//<![CDATA[
(function( $ ) { 'use strict';
	
	
	
	$('input,a,select,textarea,button').removeAttr('tabindex');
	
}(jQuery));
//]]>
</script><script>
    jQuery( document ).ready( matchClass );
    
function matchClass() {
	jQuery( ".wpcf7-form-control" ).each (function() {
		if ( jQuery( this ).hasClass( "comforminline" )) {
			var ispara = jQuery(this).closest( "p" );
			ispara.addClass("comforminline");
		} else if ( jQuery( this ).hasClass( "comformblock" )) {
			var ispara = jQuery(this).closest( "p" );
			ispara.addClass("comformblock");
		}
	})
}
</script>

