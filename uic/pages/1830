	<script type="text/javascript">
jQuery(document).ready(function() {
if (jQuery('#form').length > 0) {
var validateForm = function() {
if (Validation.fieldExists('form', 'first_name', 'input')) {
/* <!-- start First Name: input -->*/
var value = Validation.getValue('form', 'first_name', 'input');
if (!Validation.required(value)) {
Validation.addMessage('form', 'first_name', 'Please enter a first name');
}
/* <!-- end First Name: input -->*/
} if (Validation.fieldExists('form', 'last_name', 'input')) {
/* <!-- start Last Name: input -->*/
var value = Validation.getValue('form', 'last_name', 'input');
if (!Validation.required(value)) {
Validation.addMessage('form', 'last_name', 'Please enter a last name');
}
/* <!-- end Last Name: input -->*/
} if (Validation.fieldExists('form', 'email', 'input')) {
/* <!-- start Primary Email: input -->*/
var value = Validation.getValue('form', 'email', 'input');
if (!Validation.required(value)) {
Validation.addMessage('form', 'email', 'Please enter a primary email');
}
/* <!-- end Primary Email: input -->*/
} if (Validation.fieldExists('form', 'user_name', 'input')) {
/* <!-- start User Name: input -->*/
var value = Validation.getValue('form', 'user_name', 'input');
if (!Validation.required(value)) {
Validation.addMessage('form', 'user_name', 'Please enter a user name');
}
if (!Validation.maxlength(value, '30')) {
Validation.addMessage('form', 'user_name', 'User name must be less than 30 characters');
}
/* <!-- end User Name: input -->*/
} if (Validation.fieldExists('form', 'password', 'input')) {
/* <!-- start Password: input -->*/
var value = Validation.getValue('form', 'password', 'input');
if (!Validation.required(value)) {
Validation.addMessage('form', 'password', 'Please enter a password');
}
/* <!-- end Password: input -->*/
} if (Validation.fieldExists('form', 'confirm_password', 'input')) {
/* <!-- start Confirm Password: input -->*/
var value = Validation.getValue('form', 'confirm_password', 'input');
if (!Validation.required(value)) {
Validation.addMessage('form', 'confirm_password', 'Please confirm your password');
}
/* <!-- end Confirm Password: input -->*/
} if (Validation.fieldExists('form', 'password_question_id', 'select')) {
/* <!-- start Password Question: select -->*/
var value = Validation.getValue('form', 'password_question_id', 'select');
if (!Validation.required(value)) {
Validation.addMessage('form', 'password_question_id', 'Please enter a hint question');
}
/* <!-- end Password Question: select -->*/
} if (Validation.fieldExists('form', 'password_answer', 'input')) {
/* <!-- start Password Answer: input -->*/
var value = Validation.getValue('form', 'password_answer', 'input');
if (!Validation.required(value)) {
Validation.addMessage('form', 'password_answer', 'Please enter a hint answer');
}
/* <!-- end Password Answer: input -->*/
}
}
Validation.addFunction('form', 'validateForm', validateForm, false);
Validation.interceptForm('form');
Validation.addRequired('first_name', 'Please enter a first name');
Validation.addRequired('last_name', 'Please enter a last name');
Validation.addRequired('email', 'Please enter a primary email');
Validation.addRequired('user_name', 'Please enter a user name');
Validation.addRequired('password', 'Please enter a password');
Validation.addRequired('confirm_password', 'Please confirm your password');
Validation.addRequired('password_question_id', 'Please enter a hint question');
Validation.addRequired('password_answer', 'Please enter a hint answer');
if(jQuery('span.required').length){
Validation.addInstructions('form');
}
}
});
