<script type="text/javascript">	
	var lookupUsernameTimeout = null;
	var lookupPasswordTimeout = null;
	var lookupEmailTimeout = null;
	var lookupEmail2Timeout = null;
	var usernameSuccess = false;
	var passwordSuccess = false;
	var emailSuccess = false;
	
	function setSubmit(){
		if (usernameSuccess == true && passwordSuccess == true){
			jQuery("#save").attr("disabled",false);
		} else {
			jQuery("#save").attr("disabled",true);
		}


		if(emailSuccess == true){
			jQuery("#save").attr("disabled",false);
		}else if($('#email_user_name').length == 1){
			jQuery("#save").attr("disabled",true);
		}
	}

	function checkUserNameAvailable(){
		clearTimeout(lookupUsernameTimeout);
		lookupUsernameTimeout = null;
		lookupUsernameTimeout = setTimeout("validateUserName()", 1000);
	}
	
	function checkPasswordQuality(){
		clearTimeout(lookupPasswordTimeout);
		lookupPasswordTimeout = null;
		lookupPasswordTimeout = setTimeout("checkPasswordsMatch()", 1000);
	}

	function checkEmailUserName(){
		clearTimeout(lookupEmailTimeout);
		lookupEmailTimeout = null;
		lookupEmailTimeout = setTimeout("checkEmailMatch()", 1000);
	}

	function checkEmailValidity(){
		clearTimeout(lookupEmail2Timeout);
		lookupEmail2Timeout = null;
		lookupEmail2Timeout = setTimeout("checkEmail()", 300);
	}
			
	function validateUserName(){
		var username = document.getElementById("user_name").value;

		if (username != ""){
					
			var url = 'controller.cfm?method=publicController.validateUserNameBeforeSubmit&returnFormat=json&user_name='+username;
					
			jQuery.ajax({
				url: url,
				dataType: 'json',
				success: function(data){
					if (data.exists == 1) {
						jQuery("#user_name").attr("style","backgroundColor: #FFF7F7;border: 1px solid #EF9292");
						usernameSuccess = false;
						setSubmit();
						jQuery("#usernameAvailable").html("<span style='color:red;'>User name is not available.</span>");
					} else {
						jQuery("#user_name").attr("style","backgroundColor: #F7FFF7;border: 1px solid #92EF92");
						usernameSuccess = true;
						setSubmit();
						jQuery("#usernameAvailable").html("<span style='color:green;'>User name is available.</span>");
					}
			}});
		} else {
			jQuery("#user_name").attr("style","backgroundColor: #FFF7F7;border: 1px solid #EF9292");
			usernameSuccess = false;
			setSubmit();
			jQuery("#usernameAvailable").html("<span style='color:red;'>Please enter a user name.</span>");
		}	
	}
	
	function checkPasswordsMatch(){
		var pass = jQuery('#password').val();
		var conf_pass = jQuery('#confirm_password').val();
		
		var url = 'controller.cfm?method=publicController.validatePasswordBeforeSubmit&returnFormat=json&password='+encodeURIComponent(pass)+'&confirm_password='+encodeURIComponent(conf_pass);
				
		jQuery.ajax({
			url: url,
			dataType: 'json',
			success: function(data){
				if (data.message != "") {
					jQuery("#passwordsMatch").html("<span style='color:red;'>"+data.message+"</span>");
					jQuery("#passwordsMatch").attr("style","display:block;padding-left:138px;");
					passwordSuccess = false;
					setSubmit();
				} else {
					jQuery("#passwordsMatch").attr("style","display:none;padding-left:138px;");
					passwordSuccess = true;
					setSubmit();
				}
		}});
		
	}

	function checkEmailMatch(){
		var email = jQuery('#email').val();
		var email_user_name = jQuery('#email_user_name').val();
		var message = "Emails do not match";
		if(Validation.email(email)==false){
			message = "Invalid email"
		}else{
			message = "Emails do not match"
		}

		if (email != email_user_name || Validation.email(email)==false){

			jQuery("#emailsMatch").html("<span style='color:red;'>"+message+"</span>");
			jQuery("#emailsMatch").attr("style","display:block;padding-left:138px;");
			emailSuccess = false;
			setSubmit();
		} else {
			jQuery("#emailsMatch").attr("style","display:none;padding-left:138px;");
			emailSuccess = true;
			setSubmit();
		}
	}

	function checkEmail(){
		var email = jQuery('#email').val();

		if(Validation.email(email)==false){
			jQuery("#emailValid").html("<span style='color:red;'>"+"Invalid email"+"</span>");
			jQuery("#emailValid").attr("style","display:block;padding-left:138px;");
			emailSuccess = false;
			setSubmit();
			return false;
		} else {
			jQuery("#emailValid").attr("style","display:none;padding-left:138px;");
			emailSuccess = true;
			setSubmit();
			return true;
		}
	}
	
	jQuery(document).ready(function() {
		checkPasswordsMatch();
		checkUserNameAvailable();
		//checkEmailUserName();
		
	});
</script>
