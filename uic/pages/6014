.k2t-footer .btn-ripple {
    cursor: pointer;
}

/* make button text white - August 17 */
.button-default span {
    color: white;
}

/*make menu bold*/
.k2t-header-mid .k2t-menu > li > a {
    font-weight: 500;
}

/* set sub-sub-menu to appear at side of submenu - Dec 12 */
.k2t-header-mid .k2t-menu > li:last-of-type .sub-menu .sub-menu {
    left: -200px;
}

/* remove excessive spacing between position and social icons on faculty page - May 18, 2017 */
.teacher-listing 
article 
.position {
    margin-bottom: 5px;
}

.teacher-listing 
article 
.social {
    padding-top: 0;
}

/* prettify social icons - May 18, 2017 */
.teacher-listing article .social a {
    background: transparent !important;
    color: #bbb !important;
    font-size: 150% !important;
}	
