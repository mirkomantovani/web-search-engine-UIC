// =======================================
// do not edit anything below this line
// =======================================

function runSlideShowh(){
   $("RadMenubar").style.display = 'block';

   var rw = document.getElementById("banner").clientWidth - document.images.Radiology.width;
   var h = 100; //document.images.Radiology.height;

   var p = $('banner');
   var a = $$('a',p);
   var c = $$('img',p);

   for(var ia=1; ia<a.length; ia++) {
      if (document.all) {
         c[ia].style.filter="blendTrans(duration=2)";
         c[ia].style.filter="blendTrans(duration=crossFadeDurationh)";
         c[ia].filters.blendTrans.Apply();
         }
      }

   for(var ia=1; ia<a.length; ia++) {
      a[ia].style.display = 'none';
      c[ia].src = "";
      }

   p1=preLoad1.length;
   var iat=0;
   for(var ia=1,ip=0; ia<a.length && ip<p1; ia++, j1=(++j1)%p1, ip++) {
      if (!preLoad1[j1].complete) continue;
      var w = preLoad1[j1].width*(h/preLoad1[j1].height);
      rw-=w;
      if (rw < 5) break;
      
      a[ia].href = url1[j1];
      c[ia].src = preLoad1[j1].src;
      c[ia].height = h;
      c[ia].title = title1[j1];
//      a[ia].style.display = 'inline';
      iat=ia;
      }

   for(var ia=1; ia<=iat; ia++) {
      a[ia].style.display = 'inline';
      if (document.all) {
         c[ia].filters.blendTrans.Play();
         }
      }

   t1 = setTimeout('runSlideShowh()', (rw>10) ? 100 : slideShowSpeedh)

}
</script>

