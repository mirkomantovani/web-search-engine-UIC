            <section role="region" aria-label="Pilsen" class="no-background">
    <div class="quad-gap-bottom">
        <div class="highlight no-background">
    <div class="ms-row zero-margin">
        <div class="l-col-1-2">
            <div class="highlight-image">
  <a href="https://www.google.com/maps/place/Pilsen,+Chicago,+IL+60608/@41.856233,-87.6563448,16z/data=!3m1!4b1!4m2!3m1!1s0x880e2cff885d6b71:0x92f41b0f7df44cf8">
    <span class="image-cell no-height" role="link">
      <img class="image smaller-image object-fit-cover zoomable" src="/_assets/cfa16da98a3e5c150aee65436ae3cf4c.svg" alt="Map of Pilsen">
