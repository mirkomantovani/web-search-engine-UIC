                    document.survey_1860547_1125T141515[name_of_field + '_other'].focus(); 
                } 
                else 
                { 
                    document.survey_1860547_1125T141515[name_of_field + '_other'].disabled=true; 
                    document.survey_1860547_1125T141515[name_of_field + '_other'].value=''; 
                } 
            } 
            function radioOther(name_of_field) 
