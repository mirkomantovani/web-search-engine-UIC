            function checkBrowserVersion() {
            var rv = -1; 
            if (navigator.appName == 'Microsoft Internet Explorer') 
            {
            var ua = navigator.userAgent;
            var re = new RegExp("MSIE ([0-9]{1,}[.0-9]{0,})");
            if (re.exec(ua) != null)
            {
            rv = parseFloat(RegExp.$1);
            }    
            }
            if (rv >= 8.0)
            {
            //alert('is IE8');
            return true;
            }
            else
            {
            //alert('is not IE8');
            return false;
            }
            }
            
            //execute on page load if IE8 or above
            var isIE8orAbove = checkBrowserVersion();
            
            if(isIE8orAbove)
            {
            var wrapperWidth = document.getElementById('form-wrapper').offsetWidth; 
            
