    <!-- Template file: sites/all/themes/custom/uicrec/templates/block/block.tpl.php -->
<div id="block-system-main" class="block block-system clearfix">

      
  <div class="container countdown" style=""position: relative;">
		  					<div id="clockdiv" class="center" style="z-index: 10000;">
								<h2><span id="header">Countdown to Commit</span><br/>
								<small id="sub-header">UIC Class of 2022</small></h2>
								<div>
								  <span class="days"></span>
								  <div class="smalltext">Days</div>
								</div>
