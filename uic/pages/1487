<button class="btn btn-default btn-sm ng-cloak" data-ng-if="!isViewingHome() && $index == 0 && filter.terms.length > 0 && (filter.roleUniqueKeys.length > 0 || filter.marketUniqueKey || filter.categoryUniqueKey)" data-ng-click="clearAllButTerms()"><strong>Clear Filters</strong></button>
<button class="btn btn-default btn-sm ng-cloak" data-ng-if="::!(isViewingHome() || isEditing() || isViewingSearchCollection() || isViewingFavoriteCollection() || isViewingCategoryCollection()) && $index == 0" data-ng-click="clearSearch(true)"><strong>Back</strong><span class="sr-only"> Home</span></button>
<button class="btn btn-default btn-sm ng-cloak" data-ng-if="::isViewingSearchCollection()" data-ng-click="backToSearchCollections()"><strong>Back</strong><span class="sr-only"> to all search results</span></button>
<button class="btn btn-default btn-sm ng-cloak" data-ng-if="::isViewingCategoryCollection()" data-ng-click="backToCategoryCollections()"><strong>Back</strong><span class="sr-only"> to category</span></button>
<button class="btn btn-default btn-sm ng-cloak" ng-if="::isViewingFavoriteCollection()" ng-click="backToFavorites()"><strong>Back</strong><span class="sr-only"> to favorites</span></button>
<span class="ng-cloak" data-ng-if="::editingMostRecent && taskCollection.uniqueKey == '_recentlyUsed_'">
<button class="btn btn-danger btn-responsive" data-ng-click="clearAllRecentlyUsed(taskCollection)" data-ng-disabled="saveFavoritesPending || saveMostRecentPending" data-on="click" data-event-category="Store" data-event-action="{{::taskCollection.name}}" data-event-label="clear all" ng-class="{'button{{settings.mainPageTheme}}': settings.mainPageTheme != 'original'}">Clear All</button>
