					<div class="et_pb_slider_container_inner">
						
						<div class="et_pb_slide_description">
							<h2 class="et_pb_slide_title">Center on Health Promotion</h2>
				<div class="et_pb_slide_content">Providing the knowledge and skills necessary for promoting independence</p></div>
							
						</div> <!-- .et_pb_slide_description -->
					</div>
				</div> <!-- .et_pb_container -->
				
