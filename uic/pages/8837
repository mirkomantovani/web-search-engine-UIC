          <div class="main_content">
            <h1 id="page-title">
              Degrees            </h1>
              <div id="block-system-main" class="block block-system">
    
  <div class="content">
    <article id="node-21" class="node node-text-page clearfix" about="/degrees-0" typeof="sioc:Item foaf:Document" role="article">



  <div class="node-content content">


    <div class="field-collection-container clearfix">
<div class="field field-name-field-content-block field-type-field-collection field-label-hidden">
    <div class="field-items">
          <div class="field-item even"><div class="field-collection-view clearfix view-mode-full field-collection-view-final"><div class="entity entity-field-collection-item field-collection-item-field-content-block clearfix" about="/field-collection/field-content-block/43" typeof="">
  <div class="content">
    <div class="title-slider">
<div class="field field-name-field-heading-images field-type-image field-label-hidden">
    <div class="field-items">
    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel" data-interval="false">


      <!-- Wrapper for slides -->
      <div class="carousel-inner">


        
        <div class="item field-item active">
          <img typeof="foaf:Image" src="http://theatreandmusic.uic.edu/sites/default/files/pages/IMG_9514.JPG" width="792" height="456" alt="Music Ensembles for Music degree" />        </div>

        <!--  end foreach($items as $delta => $item)     -->



      </div>

      <!-- Controls -->
          </div> <!-- Carousel -->

  </div><!-- div with field items and content attributes-->
