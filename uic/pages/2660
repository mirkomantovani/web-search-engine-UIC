	<script type="text/javascript" >
		function submitme() {
			var vform = document.getElementById('formbody');

			vform.action = "https://www.uif.uillinois.edu/gifts/PaymentPageTransfer.aspx";

      	return true;
      }
   </script>

   <script>
   // This will prevent a second "Other" fund from being entered

   $( "#donation-designate-submit" ).click(function() {
      if ($("#donation-results").find( '[id^="fund-custom-"]' ).length) {
         $("#donation-form-row-designate").addClass( "disabled" );
         $("#donation-designate-rule").append('Our system can currently only accept one write-in fund. If you need to write in another fund, please attempt to search for it or contact the <a href="http://advance.uic.edu">UIC Office of the Vice Chancellor for Advancement<\/a> at (312) 413-2992.');
      }
   });

   // Enter Key Override for "Add Buttons"

  $('.dollar-sign-input').keypress(function(e) {
    if (e.which == 13) {
    e.preventDefault();
    $(this).blur();
    $(this).next('input.inline').click();
    }
  });
$('#donation-designate').keypress(function(e) {
    if (e.which == 13) {
    e.preventDefault();
    $(this).blur();
    $('#donation-designate-input').click().focus();
    }
  });
   </script>

