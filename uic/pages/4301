<h1 class="orange">2012 Awards</h1>

<!-- 
<div class="orange_background">
<p></p>
</div>
-->

<div class="clear"></div>

</div><!-- End of intro text -->

</div><!-- End of intro text wrapper -->

<!-- Start of bdywrapper -->
<div class="bdywrapper">

<!-- Start of main -->
<section id="main">

<!-- Start of In This Section toggle -->
<div class="menu-toggle-container">
	<a class="menu-toggle scroll-link" href="#rightside_content">In this section</a>
