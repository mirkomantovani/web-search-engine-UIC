<script>
	(function ($) {
		$(document).ready( function() {
			$('.giving-form-add-button').each( function () {
				$(this).click( function () {
					if ($('select[data-fund-id=' + $(this).data('add-id')  + ']').val() == 'make') {
						alert('Please make a selection.');
					}
					else {
						if(Math.floor($('input[data-amount-id=' + $(this).data('add-id')  + ']').val()) == $('input[data-amount-id=' + $(this).data('add-id')  + ']').val()  && $.isNumeric($('input[data-amount-id=' + $(this).data('add-id')  + ']').val()) && ($('input[data-amount-id=' + $(this).data('add-id')  + ']').val() >= 5)) {
						
							var next_add_id = $('.giving-form-hidden-total').length + 1;

							var ind  = $('select[data-fund-id=' + $(this).data('add-id')  + ']').prop('selectedIndex');

							$('#summary-id-' + $(this).data('add-id') + ind).remove();
							
							$('#giving-form-summary-items').prepend("<div id='summary-id-" + $(this).data('add-id') + ind + "'></div>");	
							$('#summary-id-' + $(this).data('add-id') + ind).prepend("<p><a href='#' onclick='jQuery(\"#summary-id-" + $(this).data('add-id') + ind  + "\").remove(); update_total(); return false;' style='font-size:22px; font-weight:bold;'>X</a> &nbsp; &nbsp; <strong>$" + $('input[data-amount-id=' + $(this).data('add-id')  + ']').val()  + "</strong> &nbsp; " + $('select[data-fund-id=' + $(this).data('add-id')  + '] option:selected').text()  + "</p>");
							$('#summary-id-' + $(this).data('add-id') + ind).prepend("<input type='hidden' class='giving-form-hidden-total' name='GIFT_AMOUNT" + next_add_id  + "' value='" + $('input[data-amount-id=' + $(this).data('add-id')  + ']').val()  + "' />")
							$('#summary-id-' + $(this).data('add-id') + ind).prepend("<input type='hidden' class='giving-form-hidden-name' name='FUND" + next_add_id  + "' value='" + $('select[data-fund-id=' + $(this).data('add-id')  + ']').val() + "' />")
						
							$('input[data-amount-id=' + $(this).data('add-id')  + ']').val('');
		
							update_total();
						
						} else if(Math.floor($('input[data-amount-id=' + $(this).data('add-id')  + ']').val()) == $('input[data-amount-id=' + $(this).data('add-id')  + ']').val()  && $.isNumeric($('input[data-amount-id=' + $(this).data('add-id')  + ']').val()) && ($('input[data-amount-id=' + $(this).data('add-id')  + ']').val() < 5)) {
				
							alert('Please enter a valid dollar amount above $5.');

						} 
						else {
							$('input[data-amount-id=' + $(this).data('add-id')  + ']').val('');
							alert('Please enter a valid dollar amount.');
						}
					}
				});
			});	

			$('#giving-form-other-add-button').click( function() {
				if(Math.floor($('#giving-form-amount-other').val()) == $('#giving-form-amount-other').val()  && $.isNumeric($('#giving-form-amount-other').val()) && ($('#giving-form-amount-other').val() >= 5)) {
					$('#summary-id-other').remove();
							
					var next_add_id = $('.giving-form-hidden-total').length + 1;
					
					$('#giving-form-summary-items').prepend("<div id='summary-id-other'></div>");	
					$('#summary-id-other').prepend("<p><a href='#' onclick='jQuery(\"#summary-id-other\").remove(); update_total(); return false;' style='font-size:22px; font-weight:bold;'>X</a> &nbsp; &nbsp; <strong>$" + $('#giving-form-amount-other').val()  + "</strong> &nbsp; " + $('#other-text-input').val()  + "</p>");
					$('#summary-id-other').prepend("<input type='hidden' class='giving-form-hidden-total' name='GIFT_AMOUNT" + next_add_id + "' value='" + $('#giving-form-amount-other').val()  + "' />")
					$('#summary-id-other').prepend("<input type='hidden' class='giving-form-hidden-name' name='FUND" + next_add_id + "' value='" + $('#other-text-input').val() +"' />")
			
					$('#other-text-input').val('');	
					$('#giving-form-amount-other').val('');

					update_total();

				} else if (Math.floor($('#giving-form-amount-other').val()) == $('#giving-form-amount-other').val()  && $.isNumeric($('#giving-form-amount-other').val()) && ($('#giving-form-amount-other').val() < 5)) {
				
					alert('Please enter a valid dollar amount above $5.');

				} else {
					$('#giving-form-amount-other').val('');
					alert('Please enter a valid dollar amount.');
				}
			});
		});
	}(jQuery));

	function update_total() {
		var total = 0;

		jQuery('.giving-form-hidden-total').each( function () {
			total += parseInt(jQuery(this).val());
		});

		jQuery('#total-donation').text("$" + total);
	}
</script>
  </div>
</div>
  </div>

		</div>

	</div>

</div>

