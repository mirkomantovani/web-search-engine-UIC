    <script type="text/javascript">

        $(window).on("unload", function () {
            if ($("#movePreviousButton").length > 0) {
                $("#movePreviousButton").removeAttr("disabled");
            }
        });

        $(document).ready(function () {

            // add move previous button
            if ($("#movePreviousContainer").length > 0) {
                $("#movePreviousContainer").html('<button class="button" type="submit" id="movePreviousButton" data-icon="[">Previous</button>');
                $("#movePreviousButton").click(function () {
                    return movePrevious();
                });
            }
        });

        function movePrevious() {
            var continueWithSubmit = true;
            if (!$("#surveyForm").valid()) {
                continueWithSubmit = confirm("This page has invalid fields. The data on this page will be lost if you move back to the previous page.  Are you sure you wish to move back?");
            }
            if (continueWithSubmit) {
                $("#moveBackwards").val(true);
                $(":input").each(function () {
                    var name = $(this).attr("name");
                    if (name) {
                        uiuc.atlas.formbuilder.service.controlService.setRequired(name, false);
                    }
                });
            }
            return continueWithSubmit;

        }
    </script>

