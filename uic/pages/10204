        if($(".tg-dropdown-list").hasClass("show")) {
			$(".tg-dropdown-list").removeClass("show");        
          	filter.removeClass("current");
        } else {
			$(".tg-dropdown-list").addClass("show"); 
          	filter.siblings().removeClass("current");          
          	filter.addClass("current");
        }
      },5);
      
    });
    
    $(".tg-grid-area-top1 .tg-filters-holder").mouseleave(function(e){
